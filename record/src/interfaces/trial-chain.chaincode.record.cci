
message Item {
  bytes data = 1;
}

message List {
  repeated Item items = 1;
}

message EmptyParams {
}

transactions {
  void AuthorizeTrial(Item) = 1;
  void RevokeAuthorization(Item) = 2;
  void SendMessage(Item) = 3;
  void DeleteMessage(Item) = 4;
}

queries {
  Item GetRecord(EmptyParams) = 1;
  List GetMessages(EmptyParams) = 2;
  List GetAuthorizedTrials(EmptyParams) = 3;
}